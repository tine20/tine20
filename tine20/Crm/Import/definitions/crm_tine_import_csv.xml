<?xml version="1.0" encoding="UTF-8"?>
<config>
    <name>crm_tine_import_csv</name>
    <model>Crm_Model_Lead</model>
    <plugin>Crm_Import_Csv</plugin>
    <type>import</type>
    <headline>1</headline>
    <dryrun>0</dryrun>
    <delimiter>,</delimiter>
    <label>Lead CSV import</label>
    <!-- _("Lead CSV import") -->
    <description>Tine 2.0 Lead import</description>
    <extension>csv</extension>
    <autotags>
        <tag>
            <name>Import list (###CURRENTDATE###)</name>
            <!-- _("Import list (###CURRENTDATE###)") -->
            <description>Leads imported on ###CURRENTDATE### at ###CURRENTTIME### by ###USERFULLNAME###</description>
            <!-- _("Leads imported on ###CURRENTDATE### at ###CURRENTTIME### by ###USERFULLNAME###") -->
            <contexts>
                <Crm />
            </contexts>
            <type>shared</type>
        </tag>
    </autotags>
    <mapping>
        <field>
            <source>lead_name</source>
            <destination>lead_name</destination>
        </field>
        <field>
            <source>leadstate_id</source>
            <destination>leadstate_id</destination>
        </field>
        <field>
            <source>Leadstate</source>
            <destination>Leadstate</destination>
        </field>
        <field>
            <source>leadtype_id</source>
            <destination>leadtype_id</destination>
        </field>
        <field>
            <source>Leadtype</source>
            <destination>Leadtype</destination>
        </field>
        <field>
            <source>leadsource_id</source>
            <destination>leadsource_id</destination>
        </field>
        <field>
            <source>Leadsource</source>
            <destination>Leadsource</destination>
        </field>
        <field>
            <source>container_id</source>
            <destination>container_id</destination>
        </field>
        <field>
            <source>start</source>
            <destination>start</destination>
        </field>
        <field>
            <source>description</source>
            <destination>description</destination>
        </field>
        <field>
            <source>end</source>
            <destination>end</destination>
        </field>
        <field>
            <source>turnover</source>
            <destination>turnover</destination>
        </field>
        <field>
            <source>probableTurnover</source>
            <destination>probableTurnover</destination>
        </field>
        <field>
            <source>probability</source>
            <destination>probability</destination>
        </field>
        <field>
            <source>end_scheduled</source>
            <destination>end_scheduled</destination>
        </field>
        <field>
            <source>resubmission_date</source>
            <destination>resubmission_date</destination>
        </field>
        <field>
            <source>tags</source>
            <destination>tags</destination>
            <separator>;</separator>
        </field>
        <field>
            <source>attachments</source>
            <destination>attachments</destination>
        </field>
        <field>
            <source>notes</source>
            <destination>notes</destination>
        </field>
        <field>
            <source>seq</source>
            <destination>seq</destination>
        </field>
        <field>
            <source>CUSTOMER</source>
            <destination>CUSTOMER</destination>
            <relation>1</relation>
            <findExistingBy>n_family</findExistingBy>
            <related_model>Addressbook_Model_Contact</related_model>
            <degree>parent</degree>
        </field>
        <field>
            <source>PARTNER</source>
            <destination>PARTNER</destination>
            <relation>1</relation>
            <findExistingBy>n_family</findExistingBy>
            <related_model>Addressbook_Model_Contact</related_model>
            <degree>parent</degree>
        </field>
        <field>
            <source>RESPONSIBLE</source>
            <destination>RESPONSIBLE</destination>
            <relation>1</relation>
            <findExistingBy>n_family</findExistingBy>
            <related_model>Addressbook_Model_Contact</related_model>
            <degree>parent</degree>
        </field>
        <field>
            <source>TASK</source>
            <destination>TASK</destination>
            <relation>1</relation>
            <findExistingBy>summary</findExistingBy>
            <related_model>Tasks_Model_Task</related_model>
            <degree>sibling</degree>
        </field>
        <field>
            <source>PRODUCT</source>
            <destination>PRODUCT</destination>
            <relation>1</relation>
            <findExistingBy>name</findExistingBy>
            <related_model>Sales_Model_Product</related_model>
            <degree>sibling</degree>
            <separator>;</separator>
        </field>
    </mapping>
</config>

