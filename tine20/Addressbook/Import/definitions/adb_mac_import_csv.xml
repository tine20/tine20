<?xml version="1.0" encoding="UTF-8"?>
<config>
    <name>adb_mac_import_csv</name>
    <model>Addressbook_Model_Contact</model>
    <plugin>Addressbook_Import_Csv</plugin>
    <type>import</type>
    <headline>1</headline>
    <dryrun>0</dryrun>
    <delimiter>TAB</delimiter>
    <description> mac addressbook german exported by 
http://www.macupdate.com/info.php/id/9312
with all fields enabled</description>
    <encoding>MAC-CENTRALEUROPE</encoding>
    <example>Addressbook/Import/examples/adb_mac_import.csv</example>
    <extension>csv</extension>
    <label>Contact CSV import from mac address book</label>
